<UserControl x:Class="SistSeguridad.Simulator.Bateria"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SistSeguridad.Simulator"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="100">
    <Grid MaxHeight="30" MaxWidth="100">
        <CheckBox Content="Bateria" HorizontalAlignment="Center" VerticalAlignment="Center" Checked="OnBattery" Unchecked="OnBattery" />
        <Border BorderBrush="Black" BorderThickness="1"/>
    </Grid>
</UserControl>
